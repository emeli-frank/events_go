{{template "base" .}}

{{define "title"}}Home{{end}}

{{define "body"}}
<form method="POST">
    <div>
        <div>Names</div>
        <input name="names">
    </div>

    <div>
        <div>Email</div>
        <input type="email" name="email">
    </div>

    <div>
        <div>Password</div>
        <input type="password" name="password">
    </div>

    <div>
        <div>Confirm password</div>
        <input type="password" name="password-confirmation">
    </div>

    <button type="submit">Submit</button>
</form>
{{end}}